{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "lib": ["esnext", "dom"],
    "experimentalDecorators": true,
    "baseUrl": ".",
    "types": ["element-plus/global"],
    "paths": {
      "@/*": ["./src/app/*"]
    },
    "strictNullChecks": true,
  },
  "include": [
    "env.d.ts",
    "components.d.ts",
    "auto-imports.d.ts",
    "src/app/**/*",
    "src/app/*",
    "src/app/**/*.vue",
    "vite.renderer.config.ts",
    "eslint.config.ts",
  ],
  "exclude": []
}
